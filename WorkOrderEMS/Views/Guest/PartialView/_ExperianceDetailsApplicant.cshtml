}

                        
<div class="panel panel-primary setup-content">
    <div class="basic-form">
        <div class="panelData">
            <div class="form-row">
                <div class="form-group col-md-4">
                    <label class="col-form-label" for="FirstName">First Name<span class="req">*</span></label>
                    @Html.TextBox("", new { @id = "FirstName", @class = "form-control input-rounded" })
